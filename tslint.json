 {
     "defaultSeverity": "error",
     "extends": [
         "tslint:recommended"
     ],
     "rules" :{
         "indent" :[
             true,
             "spaces"
         ],
         "interface-name":[
             true,
             "always-prefix"
            ],
         "member-access":[
             true
         ]
         [
         "max-classes-per-file": false,]
         "no-empty": false,
         "object-literal-sort-keys": false,
        "no-unused-ecpression": true,
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "quotemark": false,
        "eofline": false,
        "samicolon": [
            true,
            "always"
        ],
        "no-console": false,
        "class-name": true,
        "prefer-const": true,
        "import-spacing": true,
        "ordered-imports": false,
        "trailling-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "ignore",
                    "typeLiterals": "ignore"
                }
            }
        ],
        "max-line-length": false,
     },
     "ruleDirectory": [],
     "linterOptions": {
         "exclude": [
             "e2e/**/*"
         ]
     }
 }